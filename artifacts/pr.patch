+def _normalize_iso_datetime_text(text: str) -> str:
+    match = _ISO_FRACTION_RE.match(text)
+    if match is None:
+        return text
+    fraction = match.group("fraction")
+    digits = fraction[1:]
+    if len(digits) <= 6:
+        return text
+    suffix = match.group("suffix") or ""
+    return f"{match.group('body')}.{digits[:6]}{suffix}"
